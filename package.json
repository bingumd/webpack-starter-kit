    "@babel/register": "~7.13.14",
    "autoprefixer": "^10.2.6",
    "babel-loader": "~8.2.2",
    "clean-webpack-plugin": "~4.0.0-alpha.0",
    "copy-webpack-plugin": "~9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "~5.2.6",
    "file-loader": "^6.2.0",
