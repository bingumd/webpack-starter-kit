{
  "name": "webpack-starter-kit",
  "version": "0.4.0",
  "description": "Webpack 5 starter kit",
  "keywords": [
    "webpack",
    "sass",
    "nunjucks"
  ],
  "license": "MIT",
  "author": "bingumd <bingumd@gmail.com>",
  "main": "webpack.config.babel.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development webpack serve",
    "dev": "cross-env NODE_ENV=development webpack --watch",
    "prod": "cross-env NODE_ENV=production webpack",
    "lint": "stylelint \"**/*.scss\"",
    "lint:fix": "stylelint \"**/*.scss\" --fix",
    "format": "prettier . --write",
    "update": "yarn upgrade-interactive --latest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "~7.13.10",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/preset-env": "~7.13.12",
    "@babel/register": "~7.13.8",
    "@bingumd/stylelint-config": "^0.11.0",
    "@stylelint/prettier-config": "^2.0.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "~8.2.2",
    "clean-webpack-plugin": "~3.0.0",
    "copy-webpack-plugin": "~8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "~5.1.3",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "~5.3.1",
    "is-windows": "~1.0.2",
    "mini-css-extract-plugin": "~1.3.9",
    "nunjucks": "^3.2.3",
    "pinst": "^2.1.6",
    "postcss": "~8.2.8",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-loader": "~5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "sass": "~1.32.8",
    "sass-loader": "~11.0.1",
    "simple-nunjucks-loader": "^3.1.0",
    "style-loader": "~2.0.0",
    "stylelint": "^13.12.0",
    "svg-spritemap-webpack-plugin": "^3.9.1",
    "terser-webpack-plugin": "~5.1.1",
    "webpack": "~5.27.2",
    "webpack-cli": "~4.5.0",
    "webpack-dev-server": "~3.11.2",
    "webpack-merge": "~5.7.3"
  }
}
